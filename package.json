{
  "name": "Atup",
  "version": "1.0.0",
  "homepage": "./",
  "main": "./public/electron.js",
  "private": true,
  "author": "Atup Development Team",
  "description": "A full featured dredging, sales, and account management application",
  "build": {
    "appId": "com.atup.TheBeetleTaxis.atup.dredge",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "./node_modules/.bin/electron-builder -c.extraMetadata.main=build/electron.js",
    "preelectron-pack": "npm run-script build"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.1",
    "concurrently": "^6.0.2",
    "cross-env": "^7.0.3",
    "electron-debug": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "html2canvas": "^1.0.0-rc.7",
    "jspdf": "^2.3.1",
    "md5": "^2.3.0",
    "moment": "^2.29.1",
    "node-sass": "^5.0.0",
    "pullstate": "^1.22.1",
    "pushmebyx": "^1.14.0",
    "react": "^17.0.2",
    "react-accessible-accordion": "^3.3.4",
    "react-dom": "^17.0.2",
    "react-loading-skeleton": "^2.2.0",
    "react-persistent-store-manager": "^1.1.0",
    "react-redux": "^7.2.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-scroll": "^1.8.2",
    "react-timekeeper": "^2.1.3",
    "secure-web-storage": "^1.0.2",
    "sweetalert2": "^10.16.0",
    "typescript": "^4.3.2",
    "wait-on": "^5.3.0",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@types/react-dom": "^17.0.5",
    "electron": "^12.0.6",
    "electron-builder": "^22.10.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
